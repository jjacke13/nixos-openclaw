{
  "meta": {
    "comment": "User configuration - editable via web form or manually"
  },

  "auth": {
    "profiles": {}
  },

  "models": {
    "mode": "merge",
    "providers": {
      "<provider-name>": {
        "baseUrl": "https://api.example.com",
        "apiKey": "<your-api-key>",
        "auth": "api-key",
        "api": "openai-completions",
        "models": [
          {
            "id": "<model-id>",
            "name": "<Model Display Name>",
            "reasoning": false,
            "input": ["text"],
            "cost": {
              "input": 0.3,
              "output": 1,
              "cacheRead": 0,
              "cacheWrite": 0
            },
            "contextWindow": 200000,
            "maxTokens": 8192
          }
        ]
      }
    }
  },

  "agents": {
    "defaults": {
      "model": {
        "primary": "<provider>/<model-id>"
      },
      "models": {
        "anthropic/claude-sonnet-4-5": { "alias": "sonnet" },
        "anthropic/claude-opus-4-5": { "alias": "opus" },
        "<provider>/<model-id>": { "alias": "<short-alias>" }
      },
      "memorySearch": {
        "enabled": true,
        "sources": ["memory", "sessions"],
        "experimental": {
          "sessionMemory": true
        },
        "provider": "local",
        "local": {
          "modelPath": "<path-to-embedding-model.gguf>"
        },
        "cache": {
          "enabled": true,
          "maxEntries": 50000
        }
      },
      "contextPruning": {
        "mode": "cache-ttl",
        "ttl": "1h"
      },
      "compaction": {
        "mode": "safeguard"
      },
      "heartbeat": {
        "every": "4h",
        "model": "<provider>/<cheap-model>"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    },
    "list": [
      {
        "id": "main",
        "default": true
      },
      {
        "id": "<agent-id>",
        "model": {
          "primary": "<provider>/<model-id>"
        },
        "groupChat": {
          "mentionPatterns": ["@<agent-name>", "<Agent Name>"]
        }
      }
    ]
  },

  "bindings": [
    {
      "agentId": "<agent-id>",
      "match": {
        "channel": "<channel-name>",
        "accountId": "<account-id>"
      }
    }
  ],

  "messages": {
    "ackReactionScope": "group-mentions"
  },

  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },

  "channels": {
    "telegram": {
      "enabled": false,
      "botToken": "<your-telegram-bot-token>",
      "dmPolicy": "pairing",
      "groupPolicy": "allowlist",
      "allowFrom": [],
      "streamMode": "partial",
      "historyLimit": 50,
      "replyToMode": "first",
      "linkPreview": true
    }
  },

  "skills": {
    "entries": {
      "1password": { "enabled": false },
      "specialists": { "enabled": true },
      "ppq-tts": { "enabled": true },
      "weather": { "enabled": true },
      "yt-mp3-download": { "enabled": true }
    }
  },

  "plugins": {
    "entries": {
      "nextcloud-talk": { "enabled": false },
      "telegram": { "enabled": true },
      "nostr": { "enabled": false }
    }
  }
}
